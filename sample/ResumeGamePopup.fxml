<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Popup" prefHeight="700.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameOverPgController">
    <ImageView fitHeight="141.0" fitWidth="286.0" layoutX="157.0" layoutY="43.0">
        <image>
            <Image url="@../assets/logo.png" />
        </image>
    </ImageView>
    <ImageView fitHeight="50.0" fitWidth="50.0" onMouseClicked="#resumeNo" pickOnBounds="true" preserveRatio="true" x="20" y="20">
        <Image url="@../assets/homebutton.png" />
    </ImageView>
    <ImageView fitHeight="206.0" fitWidth="285.0" layoutX="158.0" layoutY="350.0" style="-fx-border-color: white;">
      <image>
         <Image url="@../assets/backgrn.png" />
      </image>
   </ImageView>
    <Button fx:id="yesbutton" layoutX="242.0" layoutY="449.0" minHeight="20" minWidth="20" onMouseClicked="#resumeYes" prefHeight="27.0" prefWidth="51.0" text="Yes" textFill="black">
        <font>
            <Font name="Verdana" size="10.0" />
        </font>
    </Button>
    <Button fx:id="nobutton" layoutX="312.0" layoutY="449.0" minHeight="20" minWidth="20" onMouseClicked="#resumeNo" prefHeight="27.0" prefWidth="51.0" text="No" textFill="black">
        <font>
            <Font name="Verdana" size="10.0" />
        </font>
    </Button>
</AnchorPane>
